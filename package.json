{
  "name": "tolgee-import",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "A tool to import data to Tolgee platform",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "run-dev": "npm run build && node --enable-source-maps ./dist/index.js",
    "api": "openapi-generator-cli generate -g typescript-fetch --skip-validate-spec -i https://app.tolgee.io/v3/api-docs/V2%20Accessible%20with%20API%20key%20V2 -o src/generated"
  },
  "author": "Jan Cizmar",
  "license": "ISC",
  "dependencies": {
    "@types/glob": "7.2.0",
    "commander": "9.2.0",
    "form-data": "4.0.0",
    "glob": "8.0.3",
    "node-fetch": "2.6.7",
    "typescript": "4.6.4"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.1",
    "@types/node": "17.0.33",
    "@types/node-fetch": "2.6.1",
    "commander": "9.2.0",
    "form-data": "4.0.0",
    "node-fetch": "2.6.7",
    "openapi-typescript": "^5.4.1",
    "openapi-typescript-fetch": "^1.1.3",
    "typescript": "4.6.4"
  }
}
